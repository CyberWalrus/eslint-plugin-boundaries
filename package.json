{
  "name": "eslint-plugin-boundaries",
  "version": "1.1.0",
  "description": "Eslint plugin checking architecture boundaries between elements",
  "keywords": [
    "eslint",
    "plugin",
    "architecture",
    "boundaries",
    "dependencies",
    "hierarchy"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/javierbrea/eslint-plugin-boundaries",
  "files": [
    "src"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js src *.js",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:unit": "npm run test"
  },
  "peerDependencies": {
    "eslint": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "globule": "1.3.2"
  },
  "devDependencies": {
    "coveralls": "3.0.9",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.2",
    "prettier": "2.2.1"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 14.x || 15.x"
  }
}
